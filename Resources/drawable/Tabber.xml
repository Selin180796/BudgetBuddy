<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string-array name="tab_titles">
        <item>@string/tab1_title</item>
        <item>@string/tab2_title</item>
    </string-array>

    <drawable name="icon_tab1">icon_tab1.webp</drawable>
    <drawable name="icon_tab2">icon_tab2.jpeg</drawable>

    <layout name="tabbar">
        <tab>
            <icon>@drawable/icon_tab1</icon>
            <title>@string/tab1_title</title>
        </tab>
        <tab>
            <icon>@drawable/icon_tab2</icon>
            <title>@string/tab2_title</title>
        </tab>
    </layout>
</resources>

